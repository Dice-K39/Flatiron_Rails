<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist Name</th>
            <th>Genre</th>
            <th>Release Year</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.genre %></td>
                <td><%= song.release_year %></td>
            </tr>
            <tr>
                <%= link_to 'View', song_path(song) %>
                <%= link_to 'Edit', edit_song_path(song) %>
                <%= link_to 'Delete', song %>
            </tr>
        <% end %>
    </tbody>
</table>